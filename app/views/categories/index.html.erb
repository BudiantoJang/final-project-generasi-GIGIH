<h1>All categorys</h1>

<%= link_to("Make new category", "/categories/new")%>
<% @categories.each do |category|%>
  <div class = "category-content">
    <h2> ID :<%= category.id %> </h2>
    <p> Name :<%= category.name %> </p>
    <%= button_to("Delete", "/categories/#{category.id}/destroy")%>
  </div>
<%end%>
